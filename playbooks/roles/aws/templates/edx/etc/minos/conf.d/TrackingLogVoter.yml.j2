TrackingLogVoter:
  config:
    aws_profile: !!null
    local_directory: '{{ COMMON_LOG_DIR }}/tracking'
    s3_bucket: 'edx-{{ COMMON_ENVIRONMENT }}-{{ COMMON_DEPLOYMENT }}'
    bucket_path: 'logs/tracking/{{ ansible_ec2_security_groups }}/{{ ansible_ec2_instance_id }}-{{ ansible_ec2_local_ipv4 }}'
